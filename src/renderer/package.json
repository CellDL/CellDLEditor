{
    "name": "@abi-software/celldl-editor",
    "author": {
        "name": "David Brooks",
        "email": "d.brooks@auckland.ac.nz"
    },
    "bugs": {
        "url": "https://github.com/CellDL/CellDLEditor/issues"
    },
    "description": "A Web-based editor for CellDL files.",
    "files": [
        "dist",
        "README.md",
        "LICENSE.txt"
    ],
    "keywords": [
        "vue",
        "component",
        "celldl",
        "bg-rdf"
    ],
    "license": "Apache-2.0",
    "module": "dist/CellDLEditor.es.js",
    "productName": "CellDLEditor",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/CellDL/CellDLEditor.git"
    },
    "type": "module",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/CellDLEditor.es.js"
        },
        "./style.css": "./dist/CellDLEditor.css"
    },
    "version": "0.20251031.0",
    "scripts": {
        "build": "vite build",
        "build:lib": "vite build --config vite.lib.config.ts && cp index.d.ts dist/index.d.ts",
        "clean": "bun scripts/clean.js",
        "dev": "vite dev",
        "format": "bunx --bun biome format --fix --max-diagnostics=none",
        "format:check": "bunx --bun biome format --max-diagnostics=none",
        "lint": "bunx --bun biome lint --fix --error-on-warnings --max-diagnostics=none",
        "start": "bun run build && vite preview",
        "version:new": "bun scripts/version.js"
    },
    "peerDependencies": {
        "vue": "3.4.21"
    },
    "dependencies": {
        "@atomic-ehr/ucum": "^0.2.5",
        "@jeswr/pretty-turtle": "^1.8.2",
        "@mathjax/src": "^4.0.0",
        "@primeuix/themes": "^1.2.5",
        "@primevue/auto-import-resolver": "4.4.1",
        "@vueuse/core": "14.0.0",
        "buffer": "^6.0.3",
        "flatbush": "^4.5.0",
        "isomorphic-fetch": "^3.0.0",
        "jsonschema": "^1.5.0",
        "oxigraph": "^0.5.2",
        "primeicons": "^7.0.0",
        "primevue": "4.4.1",
        "rbush": "^4.0.1",
        "svg-intersections": "github:dbrnz/svg-intersections#fix-numbers",
        "svg-path-commander": "^2.1.11",
        "systeminformation": "^5.27.11",
        "transformation-matrix": "^3.1.0",
        "ua-parser-js": "^2.0.6",
        "vue": "3.5.22"
    },
    "devDependencies": {
        "@biomejs/biome": "^2.3.2",
        "@tailwindcss/postcss": "^4.1.16",
        "@tailwindcss/vite": "^4.1.16",
        "@types/node": "^24.9.2",
        "@types/rbush": "^4.0.0",
        "@types/svg-intersections": "^0.4.4",
        "@vitejs/plugin-vue": "^6.0.1",
        "@vue/tsconfig": "^0.8.1",
        "autoprefixer": "^10.4.21",
        "pug": "^3.0.3",
        "pug-plain-loader": "^1.1.0",
        "tailwindcss": "^4.1.16",
        "tailwindcss-primeui": "^0.6.1",
        "unplugin-vue-components": "^30.0.0",
        "vite": "^7.1.12"
    }
}
