{
    "compilerOptions": {
        "baseUrl": ".",
        "composite": true,
        "module": "esnext",
        "moduleResolution": "bundler",
        "noEmit": false,
        "strictNullChecks": true,
        "resolveJsonModule": true,
        "target": "esnext",
        "types": ["vite/client"],
        "paths": {
            "@editor/*": ["./src/CellDL/*"],
            "@oxigraph/*": ["./public/oxigraph/*"],
            "@pyodide/*": ["./public/pyodide/*"],
            "@renderer/*": ["./src/*"]
        }
    },
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": ["**/*.js", "**/*.ts", "**/*.vue", "package.json"]
}
