{
    "compilerOptions": {
        "baseUrl": ".",
        "composite": true,
        "noEmit": false,
        "paths": {
            "@editor/*": ["./src/renderer/src/CellDL/*"],
            "@pyodide/*": ["./src/renderer/public/pyodide/*"],
            "@oxigraph/*": ["./src/renderer/src/assets/oxigraph/*"],
            "@renderer/*": ["./src/renderer/src/*"]
        }
    },
    "extends": "@vue/tsconfig/tsconfig.dom.json",
    "include": [
        "src/renderer/src/env.d.ts",
        "src/renderer/src/**/*",
        "src/preload/*.d.ts"
    ]
}
